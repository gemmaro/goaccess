#, no-wrap
msgid "B<192.1.2.12,\\~192.68.33.17>"
msgstr ""

#. type: tbl table
#: ../goaccess.1:981
#, no-wrap
msgid "~h{, }"
msgstr ""

#. type: tbl table
#: ../goaccess.1:984
#, no-wrap
msgid "B<192.1.2.14\\~192.68.33.17\\~192.1.1.2>"
msgstr ""

#. type: tbl table
